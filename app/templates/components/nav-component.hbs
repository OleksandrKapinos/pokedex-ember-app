<div class="pokemon-list-items">
    <div class="center">
        <button class="button-circle center"
            {{action previousPage}}
                disabled={{previousPageButtonIsDisabled}}
                aria-label="go to previous page"
        >
            <svg xmlns="http://www.w3.org/2000/svg"
                 width="48"
                 height="28"
                 viewBox="0 0 48 48"
                 data-ember-extension="1">
                <path d="M30.83 14.83L28 12 16 24l12 12 2.83-2.83L21.66 24z"/>
            </svg>
        </button>
    </div>
    <div class="center">
        <button class="button-circle center"
            {{action nextPage}}
                disabled={{nextPageButtonIsDisabled}}
                aria-label="go to next page"
        >
            <svg xmlns="http://www.w3.org/2000/svg"
                 width="48"
                 height="28"
                 viewBox="0 0 48 48">
                <path d="M20 12l-2.83 2.83L26.34 24l-9.17 9.17L20 36l12-12z"/>
            </svg>
        </button>
    </div>
</div>
